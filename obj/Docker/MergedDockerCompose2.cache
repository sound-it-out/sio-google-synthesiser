networks:
  default:
    external: true
    name: sio.network
services:
  sio.google.synthesizer:
    build:
      context: D:\Git\sound-it-out\sio-google-synthesiser
      dockerfile: src/SIO.Google.Synthesizer/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: SIO.Google.Synthesizer
      target: base
    container_name: SIO.Google.Synthesizer
    depends_on:
      sio.google.synthesizer.sql:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionStrings__GoogleSythensizerStore: Server=sio.google.synthesizer.sql;Initial
        Catalog=sio-google-sythensizer-store;User Id=sa;Password=1qaz-pl,
      ConnectionStrings__Projection: Server=sio.google.synthesizer.sql;Initial Catalog=sio-google-sythensizer-projections;User
        Id=sa;Password=1qaz-pl,
      ConnectionStrings__Store: Server=sio.sql.store;Initial Catalog=sio-store;User
        Id=sa;Password=1qaz-pl,
      DOTNET_ENVIRONMENT: Development
      Logging__LogLevel__Default: Information
      Logging__LogLevel__Microsoft: Warning
      Logging__LogLevel__Microsoft.Hosting.Lifetime: Information
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    external_links:
    - sio.sql.store
    image: siogooglesynthesizer:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/SIO.Google.Synthesizer.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    tty: true
    volumes:
    - D:\Git\sound-it-out\sio-google-synthesiser\src\SIO.Google.Synthesizer:/app:rw
    - C:\Users\matth\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\matth\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\matth\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\matth\.nuget\packages:/root/.nuget/packages:ro
  sio.google.synthesizer.sql:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: 1qaz-pl,
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1458
      target: 1433
    user: root
    volumes:
    - /var/opt/mssql/data
version: '3.4'